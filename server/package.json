{
  "name": "saraswathi-backend",
  "version": "1.0.0",
  "description": "Backend API for Saraswathi manuscript repository",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "server": "tsx watch src/index.ts",
    "delete:manuscripts": "tsx ./src/scripts/deleteAllManuscripts.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "manuscript",
    "api",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/vision": "^4.3.3",
    "@google/generative-ai": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.64.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "imagekit": "^4.1.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^8.11.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.34.5",
    "tesseract.js": "^6.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.15.1",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
